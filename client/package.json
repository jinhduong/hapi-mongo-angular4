{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run main",
    "build": "ng build --prod --stats-json && ng build --prod --app app2 --stats-json && ng build --prod --app app3 --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "admin": "ng serve --app 1 --port 4201",
    "admin-spark": "ng serve --app 2 --port 4202",
    "main": "ng serve --app 0",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "build-i18n:vi": "ng build --output-path=dist/vi --aot -prod --bh /vi/ --i18n-file=src/i18n/messages.vi.xlf --i18n-format=xlf --locale=vi",
    "build-i18n:nb": "ng build --output-path=dist/nb --aot -prod --bh /nb/ --i18n-file=src/i18n/messages.nb.xlf --i18n-format=xlf --locale=nb",
    "build-i18n": "npm run build-i18n:vi && npm run build-i18n:nb",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "bundle-report-admin": "webpack-bundle-analyzer dist/admin/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.26",
    "@ngx-translate/core": "^7.0.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "@ultimate/ngxerrors": "^1.3.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "ng2-charts": "^1.6.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "webpack-bundle-analyzer": "^2.8.2"
  }
}
